#include <iostream>
using namespace std;
int main()
{
	//the program can be run without array just (number=0)
	int sum=0;
	int number[5];
	for (short i = 0; i <= 4; i++)
	{
		
		cout << "Enter number " << i + 1 << ":\n";
		cin >> number[i];
		if (number[i] > 50)
		{
			cout <<"the number must be less than or equal 50, try again\n";
			i--;
			
			continue;
		}
		
		sum+= number[i];	

	}
	cout << sum;
	return 0;
}


