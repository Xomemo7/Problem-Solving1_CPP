#include <iostream>
using namespace std;
int main()
{
    int num1,num2,num3;
    cout << "Enter 3 different numbers:\n";
    cin >> num1 >> num2 >> num3;
    if (num1 >= num2)
    {
        if (num1 != num2) 
        {
            if (num1 >= num3) {
                if (num1 != num3)
                    cout << "num1 is max";
                else
                    cout << "num1 and 3 are equaly max";
            }
            else
                cout << "num3 is max";
        }
        else
        {
            if (num2 >= num3)
            {
                if (num2 != num3)
                    cout << "num1 and 2 are equaly max";
                else
                    cout << "num1 and 2 and 3 are equaly max";
            }
            else
                if (num2 != num3)
                cout << "num3 is max";
                else
                cout << "num2 and 3 are equaly max";
        }
    }
    else
    {
        if (num2 > num3)
            cout << "num2 is max";
        else
            if(num2!=num3)
            cout << "num3 is max";
            else
                cout << "num2 and 3 are equaly max";
    }
    return 0;
}


